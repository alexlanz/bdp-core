pipeline {
    agent any

    stages {
        stage('Deploy dto') {
            steps {
		sh 'cd dto/'
		sh 'mvn clean install deploy'
            }
        }
        stage('Deploy dc-interface') {
            steps {
		sh 'cd dc-interface/'
		sh 'mvn clean install deploy'
            }
        }
        stage('Deploy ws-interface') {
            steps {
		sh 'cd ws-interface/'
		sh 'mvn clean install deploy'
            }
        }
    }
}
