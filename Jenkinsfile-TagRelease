pipeline {
    agent any

    stages {
        stage('Deploy dto') {
            steps {
		cd dto/
		mvn clean install deploy
            }
        }
        stage('Deploy dc-interface') {
            steps {
		cd dc-interface/
		mvn clean install deploy
            }
        }
        stage('Deploy ws-interface') {
            steps {
		cd ws-interface/
		mvn clean install deploy
            }
        }
    }
}
